<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="covidmanagementfxmlapplication.DoctorController">
   <children>
      <Label layoutX="335.0" layoutY="38.0" prefHeight="51.0" prefWidth="120.0" text="Doctor ">
         <font>
            <Font name="Poor Richard" size="43.0" />
         </font>
      </Label>
      <Button fx:id="newPatient" layoutX="340.0" layoutY="153.0" mnemonicParsing="false" onAction="#newPatientOnAction" style="-fx-background-color: #C5B4E3;" text="New Patient Entry" />
      <Button fx:id="bookOperationSchedule" layoutX="320.0" layoutY="241.0" mnemonicParsing="false" onAction="#bookOperationScheduleOnAction" style="-fx-background-color: #ffcccb;" text="Book Operation Schedule" />
      <Button fx:id="returnBack" layoutX="514.0" layoutY="354.0" mnemonicParsing="false" onAction="#returnBackOnAction" text="Return" />
      <ImageView fitHeight="361.0" fitWidth="255.0" layoutX="24.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@doctor.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
