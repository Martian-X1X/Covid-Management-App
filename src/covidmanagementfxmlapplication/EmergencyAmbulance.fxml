            <Font size="18.0" />
         </font>
      </Button>
      <Text fill="#ee0000" layoutX="338.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Ambulance">
         <font>
            <Font name="Bookman Old Style Bold" size="29.0" />
         </font>
      </Text>
      <Button fx:id="Return" layoutX="494.0" layoutY="337.0" mnemonicParsing="false" onAction="#ReturnOnAction" prefHeight="34.0" prefWidth="70.0" text="Return" />
      <ImageView fitHeight="376.0" fitWidth="245.0" layoutX="22.0" layoutY="16.0" pickOnBounds="true" preserveRatio="true">
