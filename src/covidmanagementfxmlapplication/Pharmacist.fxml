            <Font name="Forte" size="39.0" />
         </font>
      </Label>
      <ImageView fitHeight="103.0" fitWidth="114.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pharmacist.png" />
